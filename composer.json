{
	"name": "icybee/module-pages",
	"type": "icanboogie-module",
	"description": "Manages Icybee pages",
	"keywords": [
		"page",
		"sitemap"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/module-pages/issues",
		"source": "https://github.com/Icybee/module-pages"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": "^5.6 || ^7.0",
		"icanboogie/module-installer": "^4.0",
		"icanboogie/bind-render": "^0.6",
		"icybee/module-nodes": "^4.0"
	},
	"require-dev": {
		"brickrouge/brickrouge": "^3.0",
		"icybee/patron": "^2.0",
		"icybee/patron-render-support": "^0.3"
	},
	"autoload": {
		"psr-4": {
			"Icybee\\Modules\\Pages\\": "lib"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/Application.php"
		]
	},
	"scripts": {
		"post-autoload-dump": "ICanBoogie\\Autoconfig\\Hooks::on_autoload_dump"
	}
}

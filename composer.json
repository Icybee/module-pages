{
	"name": "icybee/module-pages",
	"type": "icanboogie-module",
	"description": "Manages Icybee pages",
	"keywords": [
		"page",
		"sitemap"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/module-pages/issues",
		"source": "https://github.com/Icybee/module-pages"
